      "exposed" : false
    },
    "totalData" : {
      "type" : "url",
      "value" : [
        "../API/bdm/businessData/{{tableProperties.bdm}}?q={{tableProperties.queryTotal}}&p=0&c=1"
      ],
      "exposed" : false
    },
    "watcher" : {
      "type" : "expression",
      "value" : [
        "if($data.deviceLength<768 && $data.pagination.view==\"table\"){",
        "    $data.pagination.view=\"list\";",
        "}",
        "",
        "$data.apiData = $data.loadData();",
        "",
        "if(!$data.apiData){$data.tableData=[]}",
        "",
        "if($data.tableProperties.pagination.view != $data.pagination.view || $data.tableProperties.pagination.c != $data.pagination.c){",
        "    $data.newPagination($data.pagination);",
        "}",
        "",
        "$data.tableProperties.pagination.totalRow = ($data.tableProperties.pagination.p+1)*$data.tableProperties.pagination.c;",
        "if($data.tableProperties.pagination.totalRow > $data.totalData[0]){",
        "    $data.tableProperties.pagination.totalRow = $data.totalData[0];",
        "}"
      ],
      "exposed" : false
    },
    "newPagination" : {
      "type" : "expression",
      "value" : [
        "return function(pagination){",
        "    ",
        "    $data.pagination.p = 0;",
        "    $data.pagination.s = \"\";",
        "    ",
        "    $data.tableProperties.pagination.p = pagination.p;",
        "    $data.tableProperties.pagination.c = pagination.c;",
        "    $data.tableProperties.pagination.view = pagination.view;",
        "    $data.tableProperties.pagination.s = \"\";",
        "    ",
        "};"
      ],
      "exposed" : false
    },
    "view" : {
      "type" : "constant",
      "value" : [
        "table"
      ],
      "exposed" : false
    },
    "pagination" : {
      "type" : "json",
      "value" : [
        "{",
        "    \"p\":0,",
        "    \"c\":25,",
        "    \"view\":\"table\",",
        "    \"s\":\"\",",
        "    \"totalRow\":0",
        "}"
      ],
      "exposed" : false
    },
    "loadData" : {
      "type" : "expression",
      "value" : [
        "return function(){",
        "",
        "var api = \"\",",
        "    prefix = $data.tableProperties.bdm+\"?p=\"+$data.tableProperties.pagination.p;",
        "",
        "if($data.tableProperties.pagination.view == \"card\" || $data.tableProperties.pagination.view == \"list\"){",
        "    api = prefix+\"&q=\"+$data.tableProperties.query+\"&c=\"+$data.tableProperties.pagination.c;",
        "}",
        "else if($data.tableProperties.pagination.view == \"table\" && $data.tableProperties.tableMode == \"variable\"){",
        "    api = prefix+\"&q=\"+$data.tableProperties.query+\"&c=\"+$data.totalData[0];",
        "}",
        "",
        "return api;",
        "",
        "};"
      ],
      "exposed" : false
    },
    "apiData" : {
      "type" : "constant",
      "exposed" : false
    },
    "listView" : {
      "type" : "expression",
      "value" : [
        "var listVw={};",
        "",
        "if(window.innerWidth<768){",
        "    listVw={label:[",
        "        '<i class=\"fas fa-th-large\"></i>',",
        "        '<i class=\"fas fa-list\"></i>'",
        "        ],",
        "        value:[\"card\",\"list\"]",
        "    };",
        "}",
        "else{",
        "    listVw={label:[",
        "        '<i class=\"fas fa-th-large\"></i>',",
        "        '<i class=\"fas fa-list\"></i>',",
        "        '<i class=\"fas fa-table\"></i>'",
        "        ],",
        "        value:[\"card\",\"list\",\"table\"]",
        "    };",
        "}",
        "    ",
        "return listVw;"
      ],
      "exposed" : false
    },
    "deviceLength" : {
      "type" : "expression",
      "value" : [
        "return window.innerWidth;"
      ],
      "exposed" : false
    }
  },
